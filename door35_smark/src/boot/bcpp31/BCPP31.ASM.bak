_EXITEND_	ends


;
; These are the segment definitions for class DATA, BSS, BSSEND, and STACK.
; Class DATA is special since offset 0 within it is potentially a NULL pointer.
; Class BSSEND is used so we can compute the size of class BSS here in the
; startup code to know how much we must zero out.  Class BSSEND will also figure
; in the definition of a near heap, if dynamic memory management support is
; included in the application.
;

_DATA	segment
LabelB	idata				; Mark the start of the DATA class
null	dw	055aah			; NULL address (within the segment)
_DATA	ends

_BSS	segment
LabelB	bdata				; Mark the start of the BSS class
_BSS	ends

_BSSEND segment				; BSS class end marker
public	EDATA
	dw	?			; Give a fixed size for alignment
EDATA	label	word			; Mark the end of the BSS class
_BSSEND ends

_STACK	segment
public	tos				; Make the TOS visible to debuggers and ICE
IFDEF	STKSIZE
	db	STKSIZE dup (?)		; Setup the stack
_DATA	segment
GlobalW	_stklen, STKSIZE		; Needed for stack checking logic
_DATA	ends
ELSE
	db	8192 dup (?)	 	; Declare the stack size (in words)
_DATA	segment
GlobalW	_stklen, 8192			; Needed for stack checking logic
_DATA	ends
	SHOW	<No stack size specified: stack size set to 8KB>
ENDIF
	even				; Force word alignment
tos	label	word			; Define the top of stack (the initial SP value)
_STACK	ends


;
; These are segment definitions for the classes ROMDATA and ENDROMDATA.  Class
; ROMDATA never contains anything (it is filled in by a Paradigm LOCATE DUP
; directive).  Class ENDROMDATA is used to mark the end of class ROMDATA and
; prevent a segment alias (since class ROMDATA will have zero length).
;
; NOTE: You can't take the address of segments in class ENDROMDATA as they are
;       aliased with class ROMDATA.
;

_rd	segment
LabelB	ridata				; Mark the start of class ROMDATA
_rd	ends
_erd	segment
	db	16 dup (?)		; Force the next segment to a new paragraph
_erd	ends

